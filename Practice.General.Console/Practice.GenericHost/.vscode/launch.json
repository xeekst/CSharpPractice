    "version": "0.2.0",
    "configurations": [
        {
            "name": "Practice.GenericHost (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/Practice.GenericHost.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "env": {
                "DOTNET_Environment": "Production"
            },
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}